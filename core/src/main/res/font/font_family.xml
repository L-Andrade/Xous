<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <font
        android:fontStyle="normal"
        android:fontWeight="300"
        android:font="@font/worksans_thin"
        app:fontStyle="normal"
        app:fontWeight="300"
        app:font="@font/worksans_thin" />
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/worksans_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/worksans_regular" />
    <font
        android:fontStyle="italic"
        android:fontWeight="400"
        android:font="@font/worksans_italic"
        app:fontStyle="italic"
        app:fontWeight="400"
        app:font="@font/worksans_italic" />
    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/worksans_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/worksans_bold" />
</font-family>